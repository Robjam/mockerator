{
    "name": "mockerator",
    "author": "",
    "description": "Stricli command line application",
    "license": "MIT",
    "type": "module",
    "version": "0.0.0",
    "files": [
        "dist"
    ],
    "bin": {
        "mockerator": "dist/cli.js",
        "__mockerator_bash_complete": "dist/bash-complete.js"
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "debug": "tsx src/bin/cli.ts",
        "test": "vitest",
        "prebuild": "tsc -p src/tsconfig.json",
        "build": "tsup",
        "prepublishOnly": "npm run build",
        "postinstall": "npx @stricli/auto-complete@latest install mockerator __mockerator_bash_complete"
    },
    "tsup": {
        "entry": [
            "src/bin/cli.ts",
            "src/bin/bash-complete.ts"
        ],
        "format": [
            "esm"
        ],
        "tsconfig": "src/tsconfig.json",
        "clean": true,
        "splitting": true,
        "minify": true
    },
    "dependencies": {
        "@faker-js/faker": "^9.3.0",
        "@stricli/auto-complete": "^1.1.0",
        "@stricli/core": "^1.1.0",
        "ts-morph": "^24.0.0"
    },
    "devDependencies": {
        "@types/node": "22.x",
        "tsup": "^6.7.0",
        "tsx": "^4.19.2",
        "typescript": "5.6.x",
        "vitest": "^2.1.8"
    }
}